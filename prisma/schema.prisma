generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  senha    String
}

model Task {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  titulo    String
  categoria String
  createdBy String
}

